<h1>Edit Your Listing</h1>

<%= form_for @listing do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :value => @listing.name %> <br />
  <%= f.label :cost %>
  <%= f.text_field :email, :value => @listing.cost %> <br />
  <%= f.label :notes %>
  <%= f.text_field :notes, :value => @user.location %> <br />
  <%= f.submit %>
<% end %>
